{
  "buildCommand": "cd frontend/app && npm run build",
  "outputDirectory": "frontend/app/dist",
  "installCommand": "cd frontend/app && npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://your-backend-url.com"
  }
}